<?xml version="1.0" encoding="UTF-8"?>
<mod>
    <action name="hit">
        <object name="subject"/>
        <object name="tool"/>
        <object name="target"/>
        
        <code>
target.changeNumericAttr('hp', -1)
info('hp: {0}'.format(target.attr('hp')))
        </code>
    </action>
</mod>
